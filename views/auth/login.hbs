<div id="form">
  <h2>Log In</h2>
  <form action="/login" method="POST">
    <label> Email:
      <input type="email" name="email" placeholder="example@ironhack.com" />
      *
    </label>
    <br />
    <label> Password:
      <input type="password" name="password" placeholder="********" id="signup-password" />
      *
    </label>
    
    <button type="submit">Log In</button>
 
    {{#if msg}}
      <p class="error">{{msg}}</p>
    {{/if}}
  </form>
</div>